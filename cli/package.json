{
  "name": "cli",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.mjs",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "node build.mjs",
    "build:dev": "NODE_ENV=development node build.mjs --dev && NODE_OPTIONS=--enable-source-maps node dist/cli-dev.js",
    "test": "prettier --check . && xo && ava"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ink": "^4.1.0",
    "meow": "^11.0.0",
    "react": "^18.2.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/node": "^22.15.16",
    "@types/react": "^18.0.32",
    "@vdemedes/prettier-config": "^2.0.1",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "esbuild": "^0.25.4",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "xo": "^0.53.1"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  }
}
